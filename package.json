{
  "name": "osumi-angular-tools",
  "version": "1.0.10",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "build:library": "cd projects\\osumi-angular-tools && npm run build",
    "build:schematics": "cd projects\\osumi-angular-tools && tsc -p tsconfig.schematics.json",
    "copy-schematics-json": "cd projects\\osumi-angular-tools && copy schematics\\collection.json dist\\schematics && copy schematics\\ng-add\\schema.json dist\\schematics\\ng-add && copy schematics\\ng-update\\schema.json dist\\schematics\\ng-update",
    "copy-schematics": "xcopy projects\\osumi-angular-tools\\dist\\schematics dist\\osumi-angular-tools\\schematics /e /y /i",
    "clean": "cd projects\\osumi-angular-tools && rmdir /s /q dist",
    "build-and-package": "npm run build:library && npm run build:schematics && npm run copy-schematics-json && npm run copy-schematics && npm run clean"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.3",
    "@angular/cdk": "^19.0.2",
    "@angular/common": "^19.0.3",
    "@angular/compiler": "^19.0.3",
    "@angular/core": "^19.0.3",
    "@angular/forms": "^19.0.3",
    "@angular/material": "^19.0.2",
    "@angular/platform-browser": "^19.0.3",
    "@angular/platform-browser-dynamic": "^19.0.3",
    "@angular/router": "^19.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.4",
    "@angular-devkit/schematics": "^19.0.4",
    "@angular/cli": "^19.0.4",
    "@angular/compiler-cli": "^19.0.3",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "19.0.1",
    "eslint": "^9.15.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.0.1",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.16.0"
  }
}
